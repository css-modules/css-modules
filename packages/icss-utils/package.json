{
  "name": "icss-utils",
  "version": "4.0.0",
  "description": "ICSS utils for postcss ast",
  "main": "src/index.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest --coverage",
    "precommit": "lint-staged",
    "prepublish": "yarn test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "es6": true
    },
    "extends": "eslint:recommended"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/css-modules/icss-utils.git"
  },
  "keywords": [
    "css",
    "modules",
    "icss",
    "postcss"
  ],
  "author": "Glen Maddern",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/css-modules/icss-utils/issues"
  },
  "homepage": "https://github.com/css-modules/icss-utils#readme",
  "dependencies": {
    "postcss": "^7.0.5"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^3.6.1",
    "prettier": "^1.4.4"
  }
}
